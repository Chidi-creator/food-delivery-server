{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/gateway",
  "projects": {
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "delivery-service": {
      "type": "application",
      "root": "apps/delivery-service",
      "entryFile": "main",
      "sourceRoot": "apps/delivery-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/delivery-service/tsconfig.app.json"
      }
    },
    "gateway": {
      "type": "application",
      "root": "apps/gateway",
      "entryFile": "main",
      "sourceRoot": "apps/gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/gateway/tsconfig.app.json"
      }
    },
    "location-service": {
      "type": "application",
      "root": "apps/location-service",
      "entryFile": "main",
      "sourceRoot": "apps/location-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/location-service/tsconfig.app.json"
      }
    },
    "menu-service": {
      "type": "application",
      "root": "apps/menu-service",
      "entryFile": "main",
      "sourceRoot": "apps/menu-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/menu-service/tsconfig.app.json"
      }
    },
    "notification-service": {
      "type": "application",
      "root": "apps/notification-service",
      "entryFile": "main",
      "sourceRoot": "apps/notification-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-service/tsconfig.app.json"
      }
    },
    "order-service": {
      "type": "application",
      "root": "apps/order-service",
      "entryFile": "main",
      "sourceRoot": "apps/order-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order-service/tsconfig.app.json"
      }
    },
    "user-service": {
      "type": "application",
      "root": "apps/user-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-service/tsconfig.app.json"
      }
    },
    "vendor-gateway": {
      "type": "application",
      "root": "apps/vendor-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/vendor-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vendor-gateway/tsconfig.app.json"
      }
    },
    "vendor-service": {
      "type": "application",
      "root": "apps/vendor-service",
      "entryFile": "main",
      "sourceRoot": "apps/vendor-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/vendor-service/tsconfig.app.json"
      }
    }
  }
}